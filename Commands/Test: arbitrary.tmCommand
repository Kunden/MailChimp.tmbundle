<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>beforeRunningCommand</key>
	<string>nop</string>
	<key>command</key>
	<string>options='{"red", "green", "blue"}'

res=$(osascript &lt;&lt;'AS'
    tell app "TextMate"
        activate
        choose from list $options \
            with title "Pick a Color" \
            with prompt "What color do you like?"
    end tell
AS)

echo "You selected: $res"

osascript -e 'tell app "TextMate" to activate' &amp;&gt;/dev/null &amp;</string>
	<key>input</key>
	<string>none</string>
	<key>name</key>
	<string>Test: arbitrary</string>
	<key>output</key>
	<string>openAsNewDocument</string>
	<key>uuid</key>
	<string>E981A683-1630-4FF7-A12F-9CB3DED62B06</string>
</dict>
</plist>
